set encoding iso_8859_15
set terminal postscript enhanced solid color "AvantGarde-Book" 20
set output "output_therm.ps_el_thermo"

set key off
xscale=       1.000000
xshift=0.0
xmin=       0.000000
xmax=     800.000000
set xrange [xmin*xscale-xshift:xmax*xscale-xshift]
set border lw 2
eref=0.0
fact=1.0
gfact=1.0
point_size=1.0
color_red="red"
color_green="green"
color_blue="blue"
color_cyan="cyan"
color_magenta="magenta"
color_gold="gold"
color_pink="pink"
color_black="black"
color_olive="olive"
color_brown="brown"
color_gray="gray"
color_light_blue="light-blue"
color_orange="orange"
 set xlabel "T (K)"
set ylabel "Electron energy (kJ / (N mol))"
fact=      0.13133130E+04
plot  "output_therm.dat" u ($1*xscale-xshift):($2*fact-eref)*gfact w l lw 3 lc rgb color_blue
set ylabel "Electron free energy (kJ / (N mol))"
plot  "output_therm.dat" u ($1*xscale-xshift):($3*fact-eref)*gfact w l lw 3 lc rgb color_blue
fact=      0.13133130E+07
set ylabel "Electron entropy (J / K / (N mol))"
plot  "output_therm.dat" u ($1*xscale-xshift):($4*fact-eref)*gfact w l lw 3 lc rgb color_blue
set ylabel "Electron heat capacity C_v (J / K / (N mol))"
plot  "output_therm.dat" u ($1*xscale-xshift):($5*fact-eref)*gfact w l lw 3 lc rgb color_blue
fact=      0.10000000E+01
set ylabel "Electron chemical potential {/Symbol m} (Ry)"
plot  "output_therm.dat" u ($1*xscale-xshift):($6*fact-eref)*gfact w l lw 3 lc rgb color_blue
